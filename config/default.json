{
  "quadtree": {
    "maxElementSize": {
      "width": 100,
      "height": 50
    },
    "nodeCapacity": 1
  },
  "viewport": {
    "width": 800,
    "height": 600
  },
  "puppeteer": {
    "mode": "new"
  },
  "sidebar": {
    "maxLinks": 6,
    "urlLength": 30,
    "titleLength": 60
  },
  "dwelling": {
    "dwellTime": 1500,
    "keyboardDwellTime": 1000,
    "rangeWidth": 150,
    "rangeHeight": 50,
    "tabViewScrollDistance": 10,
    "menuAreaScrollDistance": 200,
    "menuAreaScrollIntervalInMs": 300,
    "activateNavAreas": true
  },
